@{
    ViewData["Title"] = "User Claims";
    var claims = User.Claims;
}
<h1>@ViewData["Title"]</h1>

<table class="table">
    <thead>
    <tr>
        <th>Type</th>
        <th>Value</th>
        <th></th>
    </tr>
    </thead>
    <tbody>
    @foreach (var claim in claims)
        {
            
        <tr>
            <td>@claim.Type</td>
            <td>@claim.Value</td>
        </tr>
        }
    </tbody>
</table>